{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "BucketName": {
      "Description": "Name of S3Bucket",
      "Type": "String",
      "Default": "s3bucketcloudformationtutorial123"
    }
  },
  "Resources": {
    "S3Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": {"Ref": "BucketName"}
      }
    }
  },
  "Outputs": {
    "S3BucketName": {
      "Value": {"Ref": "S3Bucket"},
      "Description": "Name of S3 bucket"
    }
  }
}
